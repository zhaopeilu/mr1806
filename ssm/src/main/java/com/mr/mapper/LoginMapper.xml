<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mr.mapper.LoginMapper">
    <select id="queryLogin" parameterType="com.mr.pojo.Login" resultType="com.mr.pojo.Login">
        SELECT * from t_login where login_name=#{loginName}
    </select>
</mapper>